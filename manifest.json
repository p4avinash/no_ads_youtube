{
  "name": "No Ads Youtube - p4avinash",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Watch Youtube without ads",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": ["contextMenus", "tabs", "<all_urls>"],
  "background": {
    "page": "index.html",
    "persistent": false
  },
  "icons": {
    "32": "icon.png"
  }
}
